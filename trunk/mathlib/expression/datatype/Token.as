package mathlib.expression.datatype {	public class Token {		public var type:TokenType;		public var val:*;				public function Token(_type:TokenType, _val:* = undefined):void {			type = _type;			val = _val;		}				public function toString():String {			var desc:String = "[" + type.toString();			if(val != undefined) desc += ":" + val.toString();			return desc += "]";		}	}}